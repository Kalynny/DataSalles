	<queryString language="SQL">
		<![CDATA[SELECT `venda`.`precoTotal` total,
 	   `pessoa`.`nome` nome,
       `venda`.`horario` date,
       `venda`.`codigo` cod 
 FROM `venda`, `pessoa`, `cliente`                
WHERE 
	 cliente.codigo = venda.cliente_codigo 
	 AND  `cliente`.`pessoa_codigo` = pessoa.codigo	 
ORDER BY venda.codigo]]>
	</queryString>
