	<queryString language="SQL">
		<![CDATA[SELECT
    `produto`.`descricao` descricao,
    `compra`.`precoTotal` total,
    `itemcompra`.`quantidade` quantidade,
    `itemcompra`.`precoParcial` parcial,
    `fornecedor`.`descricao` nome,
    `compra`.`atual` DATE,
    `compra`.`codigo` cod,
    `produto`.`preco`
FROM
    `compra`,
    `itemcompra`,
    `produto`,
    `funcionario`,
    `fornecedor`
WHERE
    `compra`.codigo = `itemcompra`.`compra_codigo` AND `itemcompra`.`produto_codigo` = `produto`.`codigo` AND `fornecedor`.`codigo` = `compra`.`fornecedor_codigo`
  AND `Venda`.codigo =  $P{CODVENDA}]]>
	</queryString>
