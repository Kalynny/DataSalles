	<queryString language="SQL">
		<![CDATA[SELECT `produto`.`descricao` descricao,
 			 `venda`.`precoTotal` total,
 			   `itemvenda`.`quantidade` quantidade,
 			     `itemvenda`.`precoParcial` parcial,
 			    `pessoa`.`nome` nome
 			     ,`venda`.`horario` date 
 			     ,`venda`.`codigo` cod 
                 , `produto`.`preco`
 FROM `venda`, `itemvenda`, `produto` , `funcionario`, `pessoa`, `cliente`
WHERE `venda`.codigo = `itemvenda`.`venda_codigo`
  AND `itemvenda`.`produto_codigo` =  `produto`.`codigo`
  and `cliente`.`codigo` = `venda`.`cliente_codigo`
  and `cliente`.`codigo` = `pessoa`.`codigo`
  AND `Venda`.codigo =  $P{CODVENDA}]]>
	</queryString>
